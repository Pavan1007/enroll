 #broker_applicants_list {
    .btn.btn-primary {
      text-transform: uppercase;
      padding: 10px 20px;
      border-radius: 6px;
      font-size: 16px;
      letter-spacing: 1px;
      border-top: none;
      border-left: none;
      border-right: none;
      margin-top: 5px;
      margin-right: 5px;
  }
}

.module {
  &.broker_quote_tool {
    .plan_selectors div div {
      border: 1px solid green;
      margin-top: 3px;
      padding: 3px;
    }
    .plan_selectors div div.criteria {
      font-size: 600;
      font-size: 110%;
    }
    .plan_selectors div div.active {
      font-weight: 700;
      color: white;
      background-color: rgba(92, 184, 92, .7);
    }
    .plan_selectors div {
      float: left;
      margin-right: 10px;
      width: 90px;
    }

    [aria-expanded="true"] .glyphicon {
      -webkit-transform: rotate(90deg);
      -moz-transform: rotate(90deg);
      -ms-transform: rotate(90deg);
      -o-transform: rotate(90deg);
    }

    .edge {
      border: 2px solid gray;
    }
  }
}
